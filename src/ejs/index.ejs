              <script>
                // Hamburger nav for mobile/tablet
                var hamburgerBtn = document.querySelector('.hamburger-button');
                hamburgerBtn.onclick = function () {
                  document.querySelector('body').classList.toggle("is-open");
                }
              </script>

              <script>
                // Lazy load web font
                var webfonts = document.querySelectorAll(".webfont");
                webfonts.forEach(function (webfont) {
                  webfont.rel = "stylesheet";
                });
              </script>

              <script>
                // background parallax on scrolling
                window.addEventListener('scroll', function () {
                  var scrollPosition = window.pageYOffset;
                  var hero = document.querySelector('#hero');
                  var body = document.querySelector('body');

                  var limit = hero.offsetTop + hero.offsetHeight;

                  if (scrollPosition <= limit) {
                    body.style.backgroundPositionY = (10 * scrollPosition / limit) + '%';
                  }
                });
              </script>

              <script>
                // Image fade effect
                window.addEventListener('scroll', function () {
                  var fadeElements = document.querySelectorAll('.effect-fade');
                  fadeElements.forEach(function (elem) {
                    var elemPos = elem.offsetTop;
                    var scroll = window.pageYOffset;
                    var windowHeight = window.innerHeight;
                    if (scroll > elemPos - windowHeight / 1.5) {
                      elem.classList.add('effect-scroll');
                    }
                  });
                });
              </script>

              <script src="js/modernizr-webp.js"></script>
